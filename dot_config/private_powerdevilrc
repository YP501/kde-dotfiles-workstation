[AC][Display]
DimDisplayIdleTimeoutSec=600
TurnOffDisplayIdleTimeoutSec=660
TurnOffDisplayIdleTimeoutWhenLockedSec=20

[AC][Performance]
PowerProfile=performance

[Battery][Display]
DimDisplayIdleTimeoutSec=300
TurnOffDisplayIdleTimeoutSec=360
TurnOffDisplayIdleTimeoutWhenLockedSec=20

[Battery][Performance]
PowerProfile=power-saver

[BatteryManagement]
BatteryCriticalAction=0
BatteryLowLevel=15

[LowBattery][Display]
TurnOffDisplayIdleTimeoutWhenLockedSec=20

[LowBattery][Performance]
PowerProfile=power-saver
